<h1>Lista de Semanas</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Fecha Inicio</th>
      <th>Fecha Fin</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weeks.each do |week| %>
      <tr>
        <td><%= week.name %></td>
        <td><%= week.date_inic %></td>
        <td><%= week.date_fini %></td>
        <td><%= week.status %></td>
        <td><%= link_to 'Ver', week %></td>
        <td><%= link_to 'Editar', edit_week_path(week) %></td>
        <td><%= link_to 'Eliminar', week, method: :delete, data: { confirm: 'EstÃ¡ seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nueva semana', new_week_path %>
