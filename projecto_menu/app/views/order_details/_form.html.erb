<%= simple_form_for(@order_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :cant_dishes %>
    <%= f.input :moneda %>
    <%= f.input :parcial_price %>
    <%= f.association :order_header %>
    <%= f.association :dish %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
