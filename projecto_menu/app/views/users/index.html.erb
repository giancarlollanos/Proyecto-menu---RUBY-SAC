<h1>Lista de usuarios</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>DNI</th>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Correo</th>
      <th>Direccion</th>
      <th>Celular</th>
      <th>Perfil</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
       <td><%= user.dni %></td>
       <td><%= user.name %></td>
       <td><%= user.last_name %></td>
       <td><%= user.email %></td>
       <td><%= user.address %></td>
       <td><%= user.phone %></td>
       <td><%= user.profile_id %></td>
       <td><%= link_to 'Editar', edit_user_registration_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

